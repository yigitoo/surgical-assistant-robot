<?xml version="1.0" ?>
<robot name="misa">
  <link name="world" />

  <joint name="prismatic_joint" type="prismatic">
    <parent link="world"/>
    <axis xyz = "0 0 1" />
    <child link="prismatic_joint_base"/>
    <limit effort="1000.0" lower="0" upper="0.30" velocity = "0.5"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="prismatic_joint_base">
    <visual>
      <origin rpy="0 0 -1.5708" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://misa_description/meshes/base.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <mass value = "0.125" />
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx = "0.001" iyy = "0.001" izz = "0.001" ixy = "0.001" ixz = "0.001" iyz = "0.001"/>
    </inertial>
    <collision>
      <geometry>
        <mesh filename="package://misa_description/meshes/base.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint1" type="continuous">
    <parent link="prismatic_joint_base"/>
    <child link="link_2"/>
    <origin xyz="0.0545 0 0"/>
  </joint>

  <link name="link_2">
    <visual>
      <origin rpy="0.0 1.5708 0" xyz="0.001 0 0"/>
      <geometry>
        <mesh filename="package://misa_description/meshes/common_link.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <mass value = "0.780"/>
      <origin rpy = "0.0 1.5708 0" xyz = "0.78 0 0.78"/>
      <inertia ixx = "0.006" iyy = "0.011" izz = "0.006" ixy = "-0.0001" ixz = "-0.005" iyz = "-0.00000192"/>
    </inertial>
    <collision>
      <geometry>
        <mesh filename="package://misa_description/meshes/common_link.stl"/>
      </geometry>
    </collision>

  </link>

    <joint name = "joint2" type = "continuous">
      <parent link = "link_2"/>
      <child link = "link_3"/>
      <origin xyz = "0.156051 0 0.156051"/>
      <axis xyz = "0 0 1" />
    </joint>

    <link name = "link_3">
      <visual>
        <origin rpy = "0.0 0 1.5708" xyz = "0 0 0.001"/>
        <geometry>
          <mesh filename = "package://misa_description/meshes/common_link.dae" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <inertial>
        <mass value = "0.780"/>
        <origin rpy = "0.0 0 1.5708" xyz = "0 -0.78 0.78"/>
        <inertia ixx = "0.006" iyy = "0.011" izz = "0.006" ixy = "-0.000002" ixz = "-0.005" iyz = "-0.00000192"/>
      </inertial>
      <collision>
        <geometry>
          <mesh filename="package://misa_description/meshes/common_link.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name = "joint3" type = "continuous">
      <parent link = "link_3"/>
      <child link = "link_4"/>
      <origin xyz = "0 -0.156051 0.156051"/>
      <axis xyz = "0 -1 0" />
    </joint>

    <link name = "link_4">
      <visual>
        <origin rpy = "1.5708 3.1415 0" xyz = "0 -0.001 0"/>
        <geometry>
          <mesh filename = "package://misa_description/meshes/common_link.dae" scale="0.001 0.001 0.001"/>
        </geometry>
      </visual>
      <inertial>
        <mass value = "0.780"/>
        <origin rpy = "1.5708 3.1415 0" xyz = "0.78 -0.78 0"/>
        <inertia ixx = "0.006" iyy = "0.011" izz = "0.006" ixy = "-0.000002" ixz = "-0.005" iyz = "-0.00000192"/>
      </inertial>
      <collision>
        <geometry>
          <mesh filename="package://misa_description/meshes/common_link.stl"/>
        </geometry>
      </collision>
  </link>

  <joint name = "joint4" type = "continuous">
    <parent link = "link_4"/>
    <child link = "link_5"/>
    <origin xyz = "0.156051 -0.156051 0"/>
    <axis xyz = "1 0 0" />
  </joint>

  <link name = "link_5">
    <visual>
      <origin rpy = "0 1.5708 0" xyz = "0.001 0 0"/>
      <geometry>
        <mesh filename = "package://misa_description/meshes/end_link.dae" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <inertial>
      <mass value = "0.230"/>
      <origin rpy = "0 0 0" xyz = "0.78 0.78 0" />
      <inertia ixx = "0.0001957" iyy = "0.000198" izz = "0.0001852" ixy = "0.000007265" ixz = "0.00001318" iyz = "-0.00001095"/>
    </inertial>
    <collision>
      <geometry>
        <mesh filename="package://misa_description/meshes/end_link.stl"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="prismatic_joint_base">
      <visual name="visual">
        <plugin name="set_visibility_gui" filename="libset_visibility_gui.so" />
      </visual>
  </gazebo>

</robot>
